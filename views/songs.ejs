<!DOCTYPE html>
<html>
  <% include partials/head -%>
<body>
  <% include partials/header -%>
  <% include partials/form -%>
<ol>
  <% songs.forEach(function(track){ %>
   <li>
   <p name= "song">Name: <%= track.name %> </p>
   <p name= "artist">Artist: <%= track.artists[0].name %> </p>
   <p name= "album" >Album: <%= track.album.name %></p>
   <p> <img src="<%= track.album.images[2].url %>" alt="No Image Available"> </p>
   </li>
  <%})%>
</ol>
</body>
</html>
